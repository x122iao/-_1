<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@color/ykfsdk_all_white"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/ykfsdk_ykf_xbot_form_title_padding">

    <android.widget.Button
        android:layout_marginLeft="20dp"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:id="@+id/bt_form_submit"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/ykfsdk_ykf_main_btn_bg"
        android:text="@string/ykfsdk_ykf_confirm"
        android:textColor="@color/ykfsdk_all_white"
        android:textSize="18sp" />
</RelativeLayout>