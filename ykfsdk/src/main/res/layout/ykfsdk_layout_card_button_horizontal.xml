<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:textColor="@android:color/white"
        android:gravity="center"
        android:layout_marginLeft="9dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ykfsdk_bg_card_button_bule"
        tools:text="hahahahha"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/left_button"
        app:layout_constraintRight_toLeftOf="@+id/right_buton"
        android:layout_width="0dp"
        android:layout_height="30dp"/>


    <TextView
        android:textColor="@color/ykfsdk_color_333333"
        android:gravity="center"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="9dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ykfsdk_bg_card_button_white"
        app:layout_constraintLeft_toRightOf="@id/left_button"
        app:layout_constraintRight_toRightOf="parent"
        tools:text="jijijij"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/right_buton"
        android:layout_width="0dp"
        android:layout_height="30dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>